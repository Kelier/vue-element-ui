.chapter-inline-stu {
    padding-left: 30px;
    border: 0 solid #ececec;
    align-items: center;
    height: 39px;
    line-height: 39px;
}
.inline-index-stu{
    display:inline-block;
    float:left;
}
.inline-title-stu{
    display:inline-block;
    margin-left: 30px;
}
.section-inline-stu{
    margin-left: 20px;
    margin-bottom: 10px;
    height: 25px;
    line-height: 25px;
    font-weight: 500;
    align-items: center;
    justify-content: space-around;
}
.course_catalog.stu_catalog dd.section:hover{
    background:#fff1dc ;
}
.section-inline-stu a{
    width: auto;
    height: auto;
}

.view_icon_stu,.view_code_stu,.view_preface_stu,.view_scheme_stu,.view_pro_stu,.view_exp_stu_1,.view_exp_stu{
    display: inline-block;
}
.view_icon_stu{
    float:left;
}
.view_icon_stu,.view_code_stu,.view_preface_stu{
    margin-left: 30px;
}
.view_scheme_stu,.view_upload_stu {
    min-height: 39px;
    min-width: 18px;
}
.view_exp_stu,.view_pro_stu,.view_exp_stu_1,.view_scheme_stu,.view_upload_stu{
    float: right;
    margin-right: 39px;
}
.view_exp_stu{
    margin-top: 8.5px;
}
.view_exp_stu img{
    margin-bottom: 6px;
}
.view_exp_stu_1{
    float: right;
    margin-right: 34px;
    margin-top: 7px;

}
.view_exp_stu{
    height: 25px;
    line-height: 25px;
    width: auto;
}
.view_exp_stu_1{
    background: #FEEDD1;
    padding: 0px;
    color: #ff6b1b;
    border: 1px solid #ff6b1b;
    border-radius:2px;
    height: 25px;
    line-height: 25px;
    width: auto;

}

.stu-wire {
    display: inline-block;
    background-color: #fba443;
    border: 1px solid #fba443;
    border-radius: 5px 0 0 5px;
    border-right-width: 0;
    color: #FFFFFF;
    width: 66px;
    height: 30px;
    line-height: 30px;
    text-align: center;
    margin-right: -4px;
}



.stu-wireless {
    display: inline-block;
    border: 1px solid #fba443;
    border-radius: 0 5px 5px 0;
    border-left-width: 0;
    height: 30px;
    width: 33px;
    line-height: 30px;
    text-align: center;
    color: #fba443;
    background-color: #fff

}
.card_body{
    height: 105px;
    margin-left: 15px;
    margin-top: 15px;
}
.stu_tilte_plat{
    height: 40px;
    line-height: 40px;
    margin-left: 15px;
    margin-right: 15px;
    border-bottom: 1px solid #C1C1C1;
}
.stu_tilte{
    display: inline-block;

    font-size: small;
}
.stu_title_detail{
    float: right;

    font-size: x-small;
    color: #ff6d10;
}
.catalog_tilte_stu{
    height: 50px;
    line-height: 50px;
    border-bottom: 1px solid #C1C1C1;
}
.stu_course_title{
    margin-left: 30px;
    font-size: 18px;
    font-weight: 500;
}

div.inline-index-stu,div.inline-title-stu{
    font-size: 16px;
    font-weight: 500;
}
div.view_code_stu,div.view_preface_stu{
    font-size: 14px;
    font-weight: 400;
}
.view_code_stu{
    float: left;
}
.view_preface_stu,.inline-title-stu{
    max-width:300px;
    overflow: hidden;
    text-overflow:ellipsis;
    white-space: nowrap;
}