<div id="nav">
    <div class="breadNav">
        <el-breadcrumb separator="/">
            <el-breadcrumb-item :to="{ path: '/' }">首页</el-breadcrumb-item>
            <el-breadcrumb-item>课程配置</el-breadcrumb-item>
            <el-breadcrumb-item>推荐管理</el-breadcrumb-item>
        </el-breadcrumb>
    </div>
    <div class="info_change">
        <div class="info_change">
            <el-dialog title="" :visible.sync="dialogFormVisible1" size="tiny">
                <el-form :model="form" style="margin-top: 1em;margin-right: 1em;" :rules="formrule1" ref="form">
                    <el-form-item label="推荐序号" :label-width="formLabelWidth" prop="recommendOrder">
                        <el-input v-model="form.recommendOrder" auto-complete="off"></el-input>
                    </el-form-item>

                    <!--<el-form-item label="推荐理由" :label-width="formLabelWidth">
                        <el-input v-model="form.recommendCourse" auto-complete="off"></el-input>
                    </el-form-item>-->
                </el-form>
                <div slot="footer" class="dialog-footer">
                    <el-button @click="dialogFormVisible1 = false">取 消</el-button>
                    <el-button type="primary" @click="updateRecommendState('form')">确 定</el-button>
                </div>
            </el-dialog>
            <el-dialog title="" :visible.sync="dialogFormVisible2" size="tiny">
                <el-form :model="formtea" style="margin-top: 1em;margin-right: 1em;" :rules="formrule2" ref="formtea">
                    <el-form-item label="推荐序号" :label-width="formLabelWidth" prop="recommendOrder">
                        <el-input v-model="formtea.recommendOrder" auto-complete="off"></el-input>
                    </el-form-item>

                    <el-form-item label="推荐理由" :label-width="formLabelWidth" prop="recommendCourse">
                        <el-input type="textarea" v-model="formtea.recommendCourse" auto-complete="off" :rows="4"></el-input>
                    </el-form-item>
                </el-form>
                <div slot="footer" class="dialog-footer">
                    <el-button @click="dialogFormVisible2 = false">取 消</el-button>
                    <el-button type="primary" @click="updateRecommendStateTea('formtea')">确 定</el-button>
                </div>
            </el-dialog>

        </div>
        <el-tabs v-model="activeName" @tab-click="handleClick">
            <el-tab-pane label="教师推荐" name="0">


                <div class="search_area">
                    <el-input placeholder="请输入工号" v-model="search_id">

                    </el-input>
                    <el-input placeholder="请输入姓名" class="boot" v-model="search_name">

                    </el-input>
                    <el-button type="primary" icon="search" class="boot" @click="tea_loadData(1,8)">搜索</el-button>
                </div>

                <div class="table_box">

                    <el-table :data="tableData" border style="width: 100%">

                        <el-table-column align="center" prop="id" label="序号" width="80">
                            <template scope="scope">
                                <span>{{scope.row.id}}</span>
                            </template>
                        </el-table-column>

                        <el-table-column align="center" prop="tid" label="工号" width="350">
                        </el-table-column>

                        <el-table-column align="center" prop="tname" label="姓名" width="200">
                            <template scope="scope">
                                <span>{{scope.row.tname}}</span>
                            </template>
                        </el-table-column>
                        <el-table-column align="center" prop="tsex" label="性别" width="150">
                            <template scope="scope">
                                <span>{{scope.row.tsex}}</span>
                            </template>
                        </el-table-column>
                        <el-table-column align="center" prop="tsex" label="是否推荐" width="150">
                            <template scope="scope">
                                <span>{{scope.row.is_recommend}}</span>
                            </template>
                        </el-table-column>
                        <el-table-column align="center" prop="tsex" label="推荐序号" width="200">
                            <template scope="scope">
                                <span>{{scope.row.recommend_order}}</span>
                            </template>
                        </el-table-column>
                        <el-table-column align="center" label="操作" width="200">
                            <template scope="scope">

                                <el-button type="text" icon="edit" @click="toggleTea(scope.$index,scope.row)">
                                    {{scope.row.recommend}}
                                </el-button>

                            </template>
                        </el-table-column>
                    </el-table>
                </div>

                <div align="center">
                    <div class="block">

                        <el-pagination @size-change="handleSizeChange" @current-change="handleCurrentChange"
                                       :current-page="currentPage" :page-size="pagesize"
                                       layout="total,prev, pager, next, jumper"
                                       :total="total">
                        </el-pagination>
                    </div>
                </div>

            </el-tab-pane>
            <el-tab-pane label="学生推荐" name="1">
                <div class="search_area">
                    <el-input placeholder="请输入学号" v-model="search_id">

                    </el-input>
                    <el-input placeholder="请输入姓名" v-model="search_name" class="boot">

                    </el-input>
                    <el-button type="primary" icon="search" class="boot" @click="stu_loadData(1,8)">搜索</el-button>
                </div>

                <div class="table_box">

                    <el-table :data="tableData" border style="width: 100%">
                        <el-table-column align="center" prop="id" label="序号" width="80">
                            <template scope="scope">
                                <span>{{scope.row.id}}</span>
                            </template>
                        </el-table-column>
                        <el-table-column align="center" prop="sid" label="学号" width="350">
                            <template scope="scope">
                                <span>{{scope.row.sid}}</span>
                            </template>
                        </el-table-column>
                        <el-table-column align="center" prop="sname" label="姓名" width="200">
                            <template scope="scope">
                                <span>{{scope.row.sname}}</span>
                            </template>
                        </el-table-column>
                        <el-table-column align="center" prop="sex" label="性别" width="150">
                            <template scope="scope">
                                <span>{{scope.row.sex}}</span>
                            </template>
                        </el-table-column>
                        <el-table-column align="center" prop="tsex" label="是否推荐" width="150">
                            <template scope="scope">
                                <span>{{scope.row.is_recommend}}</span>
                            </template>
                        </el-table-column>
                        <el-table-column align="center" prop="tsex" label="推荐序号" width="200">
                            <template scope="scope">
                                <span>{{scope.row.recommend_order}}</span>
                            </template>
                        </el-table-column>
                        <el-table-column align="center" label="操作" width="200">
                            <template scope="scope">

                                <el-button type="text" icon="edit" @click="toggle(scope.$index,scope.row)">
                                    {{scope.row.recommend}}
                                </el-button>
                            </template>
                        </el-table-column>
                    </el-table>
                </div>

                <div align="center">
                    <div class="block">

                        <el-pagination @size-change="handleSizeChange" @current-change="handleCurrentChange"
                                       :current-page="currentPage" :page-size="pagesize"
                                       layout="total, prev, pager, next, jumper"
                                       :total="total">
                        </el-pagination>
                    </div>
                </div>
            </el-tab-pane>
            <el-tab-pane label="开课推荐" name="2">
                <div class="search_area">

                    <el-input placeholder="请输入课程名称" v-model="coursename"></el-input>

                    <el-input placeholder="请输入教师姓名" v-model="teaname" class="boot"></el-input>

                    <el-button type="primary" icon="search" class="boot-auto" @click="sl_loadData(1,8)">搜索</el-button>
                </div>


                <div class="table_box">

                    <el-table :data="tableData" border style="width: 100%">


                        <el-table-column align="center" prop="id" label="序号" width="80">
                            <template scope="scope">

                                <span>{{scope.row.id}}</span>
                            </template>
                        </el-table-column>
                        <el-table-column align="center" prop="cid" label="课序号" width="220">
                        </el-table-column>
                        <el-table-column align="center" prop="cnum" label="课程编号" width="220">
                            <template scope="scope">
                                <span>{{scope.row.cnum}}</span>
                            </template>
                        </el-table-column>
                        <el-table-column align="center" prop="cname" label="课程名称" width="220">
                            <template scope="scope">
                                <span>{{scope.row.cname}}</span>
                            </template>
                        </el-table-column>
                        <el-table-column align="center" prop="tnum" label="教师编号" width="220">
                            <template scope="scope">
                                <span>{{scope.row.tnum}}</span>
                            </template>
                        </el-table-column>
                        <el-table-column align="center" prop="tname" label="教师姓名" width="220">
                            <template scope="scope">
                                <span>{{scope.row.tname}}</span>
                            </template>
                        </el-table-column>
                        <el-table-column align="center" prop="tsex" label="是否推荐" width="150">
                            <template scope="scope">
                                <span>{{scope.row.is_recommend}}</span>
                            </template>
                        </el-table-column>
                        <el-table-column align="center" prop="tsex" label="推荐序号" width="150">
                            <template scope="scope">
                                <span>{{scope.row.recommend_order}}</span>
                            </template>
                        </el-table-column>
                        <el-table-column align="center" label="操作" width="200">
                            <template scope="scope">

                                <el-button type="text" icon="edit" @click="toggle(scope.$index,scope.row)">
                                    {{scope.row.recommend}}
                                </el-button>

                            </template>
                        </el-table-column>
                    </el-table>
                </div>

                <div align="center">
                    <div class="block">

                        <el-pagination @size-change="handleSizeChange" @current-change="handleCurrentChange"
                                       :current-page="currentPage" :page-size="pagesize"
                                       layout="total, prev, pager, next, jumper"
                                       :total="total">
                        </el-pagination>
                    </div>
                </div>
            </el-tab-pane>
        </el-tabs>
    </div>
</div>
<script src="../js/groomLau.js"></script>